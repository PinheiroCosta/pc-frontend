{
  "compilerOptions": {
    "module": "esnext",
    "target": "ES2021",
    "allowJs": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "*": ["./frontend/*", "./frontend/js/*"]
    },
    "skipLibCheck": true,
    "strict": true,
  },
}
