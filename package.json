    "postcss": "~8.4.38",
    "postcss-loader": "~8.1.1",
    "postcss-preset-env": "^10.5.0",
    "prettier": "~3.2.5",
    "react-refresh": "^0.14.2",
    "sass": "~1.77.1",
    "sass-loader": "~14.2.1",
    "style-loader": "~4.0.0",
    "swc-loader": "^0.2.6",
    "typescript": "^5.4.5",
    "webpack": "~5.91.0",
    "webpack-bundle-tracker": "~3.1.0",
    "webpack-cli": "~5.1.4",
    "webpack-dev-server": "~5.0.4",
    "whatwg-fetch": "~3.6.20"
  }
}
