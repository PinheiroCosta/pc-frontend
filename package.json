{
  "name": "romweb-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "romweb frontend.",
  "engines": {
    "node": ">=20 <21"
  },
  "browserslist": "> 0.25%, not dead",
  "main": "frontend/js/index.tsx",
  "scripts": {
    "check": "tsc --noEmit",
    "dev": "concurrently -k -n TSC,WEBPACK -c red,cyan \"tsc --noEmit --watch\" \"webpack serve --mode=development --hot\"",
    "dev:strict": "npm run check && npm run dev",
    "build": "NODE_ENV=production tsc && webpack --progress --bail --mode=production",
    "lint": "eslint frontend",
    "lint:fix": "eslint frontend --fix",
    "format": "prettier --write frontend",
    "format:check": "prettier --check frontend",
    "tsc": "tsc -p ./tsconfig.json --noEmit",
    "openapi:download": "node scripts/openapi-download.mjs",
    "openapi:ts": "openapi-ts"
  },
  "dependencies": {
    "@hey-api/openapi-ts": "^0.45.0",
    "@sentry/browser": "~8.0.0",
    "@sentry/react": "~8.0.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "~1.12.0",
    "bootstrap": "~5.3.3",
    "classnames": "~2.5.1",
    "cookie": "~0.6.0",
    "copy-webpack-plugin": "^13.0.0",
    "he": "^1.2.0",
    "html-webpack-plugin": "^5.6.5",
    "lodash": "~4.17.21",
    "marked": "~12.0.2",
    "prismjs": "^1.30.0",
    "react": "~18.3.1",
    "react-bootstrap": "~2.10.2",
    "react-dom": "~18.3.1",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.1",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.13",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.6",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "~6.4.5",
    "@testing-library/react": "~15.0.7",
    "@testing-library/user-event": "~14.5.2",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "ajv": "~8.13.0",
    "circular-dependency-plugin": "~5.2.2",
    "concurrently": "^9.1.2",
    "css-loader": "~7.1.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-jest": "~27.9.0",
    "eslint-plugin-jsx-a11y": "~6.8.0",
    "eslint-plugin-prettier": "~5.1.3",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-react": "~7.34.1",
    "eslint-plugin-react-hooks": "~4.6.2",
    "eslint-plugin-sonarjs": "~0.23.0",
    "eslint-plugin-unicorn": "~49.0.0",
    "identity-obj-proxy": "~3.0.0",
